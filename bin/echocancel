#!/bin/bash
set -euo pipefail
echo unloading any existing modules...
pactl unload-module module-echo-cancel || true
echo loading modules...
pactl load-module module-echo-cancel aec_method=webrtc source_name=echocancel sink_name=echocancel1
# echo setting default source and sink...
# pacmd set-default-source echocancel
# pacmd set-default-sink echocancel1
echo done
